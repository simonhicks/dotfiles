"3.14.0 (created: 2016/06/07 23:58:16)

map <C-l> :nohl<Return>
set toolbars=addons,nobookmarks,nomenu,nonavigation,tabs
set! browser.fullscreen.autohide=false

set toolbars=nonavigation

map ]t gt
map [t gT
noremap [z zO
noremap ]z zI
noremap <C-t> :tab<space>

cnoremap <C-h> <left>
cnoremap <C-l> <right>
cnoremap <C-j> <down>
cnoremap <C-k> <up>

source! ~/.vimperatorrc.local
" vim: set ft=vimperator:
