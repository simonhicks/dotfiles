# short form cli apps
alias g='git'
alias s='ssh'

if [ `which xsel` ]
then
  alias pb='xsel --clipboard'
fi

if [ `which xclip` ]
then
  alias pbcopy='xclip -selection clipboard -i'
fi

if [ -e /Applications/MacVim.app/Contents/MacOS ]
then
  alias vim=/Applications/MacVim.app/Contents/MacOS/Vim
fi
